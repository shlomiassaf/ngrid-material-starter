{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ngrid-material-starter": {
      "root": "apps/ngrid-material-starter",
      "sourceRoot": "apps/ngrid-material-starter/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/apps/ngrid-material-starter",
            "index": "apps/ngrid-material-starter/src/index.html",
            "main": "apps/ngrid-material-starter/src/main.ts",
            "polyfills": "apps/ngrid-material-starter/src/polyfills.ts",
            "tsConfig": "apps/ngrid-material-starter/tsconfig.app.json",
            "assets": [
              "apps/ngrid-material-starter/src/favicon.ico",
              "apps/ngrid-material-starter/src/assets"
            ],
            "styles": ["apps/ngrid-material-starter/src/styles/main.scss"],
            "scripts": []
          },
          "configurations": {
            "viewEngine": {
              "tsConfig": "apps/ngrid-material-starter/tsconfig.app.view-engine.json"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngrid-material-starter/src/environments/environment.ts",
                  "with": "apps/ngrid-material-starter/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrid-material-starter:build"
          },
          "configurations": {
            "viewEngine": {
              "browserTarget": "ngrid-material-starter:build:viewEngine"
            },
            "production": {
              "browserTarget": "ngrid-material-starter:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrid-material-starter:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/ngrid-material-starter/src/test.ts",
            "polyfills": "apps/ngrid-material-starter/src/polyfills.ts",
            "tsConfig": "apps/ngrid-material-starter/tsconfig.spec.json",
            "karmaConfig": "apps/ngrid-material-starter/karma.conf.js",
            "styles": ["apps/ngrid-material-starter/src/styles/main.scss"],
            "scripts": [],
            "assets": [
              "apps/ngrid-material-starter/src/favicon.ico",
              "apps/ngrid-material-starter/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngrid-material-starter/tsconfig.app.json",
              "apps/ngrid-material-starter/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ngrid-material-starter-e2e": {
      "root": "apps/ngrid-material-starter-e2e",
      "sourceRoot": "apps/ngrid-material-starter-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrid-material-starter-e2e/cypress.json",
            "headless": true,
            "tsConfig": "apps/ngrid-material-starter-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngrid-material-starter:serve"
          },
          "configurations": {
            "viewEngine": {
              "devServerTarget": "ngrid-material-starter:serve:viewEngine"
            },
            "production": {
              "devServerTarget": "ngrid-material-starter:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ngrid-material-starter-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/ngrid-material-starter-e2e/**"
            ]
          }
        }
      }
    },
    "ngrid-bootstrap-starter": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ngrid-bootstrap-starter",
      "sourceRoot": "apps/ngrid-bootstrap-starter/src",
      "prefix": "ngrid-material-starter",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngrid-bootstrap-starter",
            "index": "apps/ngrid-bootstrap-starter/src/index.html",
            "main": "apps/ngrid-bootstrap-starter/src/main.ts",
            "polyfills": "apps/ngrid-bootstrap-starter/src/polyfills.ts",
            "tsConfig": "apps/ngrid-bootstrap-starter/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngrid-bootstrap-starter/src/favicon.ico",
              "apps/ngrid-bootstrap-starter/src/assets"
            ],
            "styles": ["apps/ngrid-bootstrap-starter/src/styles/main.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngrid-bootstrap-starter/src/environments/environment.ts",
                  "with": "apps/ngrid-bootstrap-starter/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrid-bootstrap-starter:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrid-bootstrap-starter:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrid-bootstrap-starter:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngrid-bootstrap-starter/tsconfig.app.json",
              "apps/ngrid-bootstrap-starter/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ngrid-bootstrap-starter/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ngrid-bootstrap-starter/jest.config.js",
            "tsConfig": "apps/ngrid-bootstrap-starter/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/ngrid-bootstrap-starter/src/test-setup.ts"
          }
        }
      }
    },
    "ngrid-bootstrap-starter-e2e": {
      "root": "apps/ngrid-bootstrap-starter-e2e",
      "sourceRoot": "apps/ngrid-bootstrap-starter-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrid-bootstrap-starter-e2e/cypress.json",
            "tsConfig": "apps/ngrid-bootstrap-starter-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngrid-bootstrap-starter:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngrid-bootstrap-starter:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ngrid-bootstrap-starter-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/ngrid-bootstrap-starter-e2e/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngrid-material-starter",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
